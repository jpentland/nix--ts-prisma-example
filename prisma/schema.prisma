generator client {
  provider = "prisma-client-js"
  binaryTargets = ["darwin", "debian-openssl-1.1.x", "rhel-openssl-1.0.x", "rhel-openssl-1.1.x", "windows"]
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Post {
  id        Int      @id @default(autoincrement())
  title     String
  content   String?
}
